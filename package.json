{
  "name": "starc-learning-platform",
  "version": "1.0.0",
  "description": "Starc Learning Platform - A complete e-learning solution",
  "scripts": {
    "start:frontend": "cd customer-client && npm run dev",
    "start:backend": "cd server && npm run dev",
    "dev": "cd customer-client && npm run dev",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "cd server && npm run dev",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "install:all": "npm install && cd customer-client && npm install && cd ../server && npm install"
  },
  "keywords": [
    "education",
    "e-learning",
    "react",
    "node",
    "express",
    "mongodb"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.32.1",
    "@types/canvas-confetti": "^1.9.0",
    "canvas-confetti": "^1.9.3"
  }
}
